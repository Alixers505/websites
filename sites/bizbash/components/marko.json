{
  "taglib-imports": [
    "./load-more/marko.json"
  ],
  "tags": {
    "bizbash-related-content": {
      "template": "./related-content.marko",
      "@content-id": {
        "type": "number",
        "required": true
      },
      "@content-type": {
        "type": "string",
        "required": true
      },
      "@section-id": {
        "type": "number",
        "required": true
      },
      "@section-name": {
        "type": "string",
        "required": true
      }
    },
    "bizbash-featured-venues-suppliers": {
      "template": "./featured-venues-suppliers.marko",
      "@content-id": {
        "type": "number",
        "required": true
      }
    },
    "bizbash-image-slider": {
      "template": "./image-slider.marko"
    },
    "bizbash-directory-search": {
      "template": "./directory-search/index.marko",
      "@search-options": "object",
      "@page-size": {
        "type": "number",
        "default-value": 25
      },
      "@topics": {
        "type": "object",
        "required": true
      },
      "@content-type": {
        "type": "string",
        "required": true
      }
    },
    "bizbash-inquiry-form": {
      "template": "./inquiry-form.marko"
    },
    "bizbash-venue-capacity": {
      "template": "./venue-capacity.marko",
      "@spaces": "array"
    },
    "bizbash-list-item": {
      "template": "./list-item.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@image": "object"
    },
    "bizbash-card-item": {
      "template": "./card-item.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@image": "object"
    },
    "bizbash-list-item-section-name": {
      "template": "./list-item-section-name.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@modifiers": "array"
    },
    "bizbash-card-item-overlay": {
      "template": "./card-item-overlay.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@image": "object"
    },
    "bizbash-overlay-item": {
      "template": "./overlay-item.marko",
      "@modifiers": "array",
      "<image>": {},
      "<header-left>": {},
      "<header-right>": {},
      "<title>": {},
      "@description <description>[]": {},
      "<footer-left>": {},
      "<footer-right>": {}
    },
    "bizbash-overlay-item-image": {
      "template": "./overlay-item-image.marko"
    },
    "bizbash-list-item-large": {
      "template": "./list-item-large.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@image": "object"
    },
    "bizbash-hero-card": {
      "template": "./hero-card.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@image": "object"
    }
  }
}
