import { getAsObject } from '@base-cms/object-path';

$ const page = getAsObject(input, 'page');

<document>
  <@head>
    <-page-metadata for="dynamic-page" ...page />
    <${input.head} />
  </@head>
  <-page-container for="dynamic-page" data=page>
    <${input.renderBody} />
  </-page-container>
</document>
