{
  "taglib-imports": [
    "./load-more/marko.json"
  ],
  "tags": {
    "mundo-related-content": {
      "template": "./related-content.marko",
      "@content-id": {
        "type": "number",
        "required": true
      },
      "@content-type": {
        "type": "string",
        "required": true
      },
      "@section-id": {
        "type": "number",
        "required": true
      },
      "@section-name": {
        "type": "string",
        "required": true
      }
    },
    "mundopmmi-featured-companies": {
      "template": "./featured-companies.marko",
      "@content-id": {
        "type": "number",
        "required": true
      }
    },
    "mundopmmi-list-item-name": {
      "template": "./list-item-name.marko",
      "@content": {
        "type": "object",
        "required": true
      },
      "@modifiers": "array"
    }
  }
}
