import { getAsArray } from '@base-cms/object-path';
import DefaultLayout from './content-layouts/default';
import ContentPageLayout from './content-layouts/content-page';

$ const layouts = {
  default: DefaultLayout,
  'content-page': ContentPageLayout,
};
$ const layout = layouts[input.name] || layouts.default;
$ const nodes = getAsArray(input, 'nodes');

<${layout} ...input nodes=nodes />
