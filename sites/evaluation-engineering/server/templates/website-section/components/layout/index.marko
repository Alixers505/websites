import { getAsObject } from '@base-cms/object-path';

$ const section = getAsObject(input, 'section');

<document>
  <@head>
    <-page-metadata for="website-section" ...section />
    <${input.head} />
  </@head>
  <-page-container for="website-section" data=section>
    <${input.renderBody} />
  </-page-container>
</document>
