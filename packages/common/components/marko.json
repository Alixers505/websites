{
  "taglib-imports": [
    "./ads/marko.json",
    "./content/marko.json",
    "./load-more/marko.json",
    "./magazine/marko.json",
    "./magazine-issue/marko.json",
    "./navbars/marko.json",
    "./magazine/marko.json",
    "./social/marko.json",
    "./subscribe/marko.json",
    "./search/marko.json"
  ],
  "tags": {
    "endeavor-element-group": {
      "template": "./element-group",
      "<header-left>": {},
      "<header-right>": {},
      "<title>": {
        "@tag": {
          "type": "string",
          "default-value": "div"
        },
        "@modifiers": "array"
      },
      "@description <description>[]": {
        "@tag": {
          "type": "string",
          "default-value": "p"
        },
        "@modifiers": "array"
      },
      "<footer-left>": {},
      "<footer-right>": {},
      "@header-modifiers": "array",
      "@footer-modifiers": "array"
    },
    "endeavor-image-element": {
      "template": "./image-element",
      "@modifiers": "array",
      "@position": {
        "type": "string",
        "default-value": "left"
      },
      "@image": {
        "type": "object",
        "required": true
      },
      "@link-to": "object",
      "@height": "number",
      "@width": "number",
      "@lazyload": {
        "type": "boolean",
        "default-value": true
      },
      "@options": "object"
    }
  }
}
