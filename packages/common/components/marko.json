{
  "taglib-imports": [
    "./ads/marko.json",
    "./content/marko.json",
    "./load-more/marko.json",
    "./magazine/marko.json",
    "./magazine-issue/marko.json",
    "./navbars/marko.json",
    "./magazine/marko.json",
    "./social/marko.json",
    "./subscribe/marko.json",
    "./search/marko.json"
  ],
  "tags": {
    "endeavor-item": {
      "template": "./item/item",
      "<image>": {
        "@display": {
          "type": "boolean",
          "default-value": true
        },
        "@image": "object",
        "@link-to": "object",
        "@height": "number",
        "@lazyload": {
          "type": "boolean",
          "default-value": true
        },
        "@modifiers": "array",
        "@options": "object",
        "@position": {
          "type": "string",
          "default-value": "left"
        },
        "@width": "number"
      },
      "<header-left>": {},
      "<header-right>": {},
      "<title>": {
        "@tag": {
          "type": "string",
          "default-value": "div"
        },
        "@modifiers": "array"
      },
      "@description <description>[]": {
        "@tag": {
          "type": "string",
          "default-value": "p"
        },
        "@modifiers": "array"
      },
      "<footer-left>": {},
      "<footer-right>": {},
      "@tag": {
        "type": "string",
        "default-value": "div"
      },
      "@header-modifiers": "array",
      "@footer-modifiers": "array",
      "@modifiers": "array",
      "@flush": {
        "type": "boolean",
        "default-value": false
      }
    },
    "endeavor-item-list": {
      "template": "./item/list",
      "<item>": {
        "@tag": {
          "type": "string",
          "default-value": "li"
        },
        "@modifiers": "array"
      },
      "<header>": {
        "@tag": {
          "type": "string",
          "default-value": "li"
        }
      },
      "@items": "array",
      "@tag": {
        "type": "string",
        "default-value": "ul"
      },
      "@flush": {
        "type": "string",
        "default-value": false
      },
      "@card": {
        "type": "string",
        "default-value": false
      },
      "@modifiers": "array"
    },
    "endeavor-item-list-wrapper": {
      "template": "./item/list-wrapper",
      "@card": {
        "type": "string",
        "default-value": false
      },
      "@flush": {
        "type": "string",
        "default-value": false
      },
      "@modifiers": "array",
      "@tag": {
        "type": "string",
        "default-value": "ul"
      }
    },
    "endeavor-item-list-header": {
      "template": "./item/list-header",
      "@modifiers": "array",
      "@tag": {
        "type": "string",
        "default-value": "li"
      }
    },
    "endeavor-item-list-node": {
      "template": "./item/list-node",
      "@modifiers": "array",
      "@node": "object",
      "@tag": {
        "type": "string",
        "default-value": "li"
      }
    },
    "endeavor-element-group": {
      "template": "./element-group",
      "<header-left>": {},
      "<header-right>": {},
      "<title>": {
        "@tag": {
          "type": "string",
          "default-value": "div"
        },
        "@modifiers": "array"
      },
      "@description <description>[]": {
        "@tag": {
          "type": "string",
          "default-value": "p"
        },
        "@modifiers": "array"
      },
      "<footer-left>": {},
      "<footer-right>": {},
      "@header-modifiers": "array",
      "@footer-modifiers": "array"
    },
    "endeavor-image-element": {
      "template": "./image-element",
      "@modifiers": "array",
      "@position": {
        "type": "string",
        "default-value": "left"
      },
      "@image": {
        "type": "object",
        "required": true
      },
      "@link-to": "object",
      "@height": "number",
      "@width": "number",
      "@lazyload": {
        "type": "boolean",
        "default-value": true
      },
      "@options": "object"
    }
  }
}
