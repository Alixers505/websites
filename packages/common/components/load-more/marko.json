{
  "tags": {
    "endeavor-load-more-header": {
      "template": "./header.marko"
    },
    "endeavor-load-more-button": {
      "template": "./button.marko",
      "@page-number": {
        "type": "number",
        "default-value": 1
      },
      "@append-to": {
        "type": "string",
        "default-value": ".container-fluid-max"
      },
      "@show": {
        "type": "boolean",
        "default-value": true
      },
      "@max-pages": "number",
      "@label": {
        "type": "string",
        "default-value": "Load More"
      },
      "@block-name": {
        "type": "string",
        "required": true
      },
      "@provide": {
        "type": "object",
        "required": true
      }
    }
  }
}
