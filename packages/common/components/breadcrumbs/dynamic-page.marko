$ const { site } = out.global;
$ const homeTitle = site.get('breadcrumbs.home.title', 'Home');
$ const homeHref = site.get('breadcrumbs.home.href', '/');

<endeavor-breadcrumbs>
  <@item>
    <endeavor-link href=homeHref title=homeTitle>${homeTitle}</endeavor-link>
  </@item>
  <@item>${input.name}</@item>
</endeavor-breadcrumbs>
