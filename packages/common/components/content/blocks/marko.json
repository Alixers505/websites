      "template": "./page-header.marko",
      "@content": {
        "type": "object",
        "required": true
      }
    },
    "endeavor-content-block-page-body": {
      "template": "./page-body.marko",
      "@content": {
        "type": "object",
        "required": true
      }
    },
    "endeavor-content-query-related": {
      "template": "./query-related.marko"
    },
    "endeavor-content-query-load-more": {
