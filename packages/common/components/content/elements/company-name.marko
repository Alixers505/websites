$ const prefixClass = `${input.block}__prefix`;

<cms-content-name
  tag=input.tag
  class=input.class
  block=input.block
  obj=input.company
  link=true
>
  <@before>
    <if(input.usePrefix)>
      <span class=prefixClass>
        ${input.prefix}
      </span>
    </if>
  </@before>
</cms-content-name>
